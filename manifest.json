{
  "manifest_version": 2,
  "name": "Kahoot, Quicker",
  "description": "Play Kahoot, Quicker",
  "version": "4",
  "author": "Arlen Yu",
  "browser_action": {
    "default_title": "Kahoot, Quicker",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://kahoot.it/*"],
      "js": ["js/kahoot.js"]
    }
  ]
}
